{
  "$schema": "https://json.schemastore.org/package",
  "name": "predator",
  "version": "1.5.3",
  "description": "Framework that manages the entire lifecycle of load testing a server, from creating test files, running scheduled and on-demand tests, and viewing test results.",
  "main": "server.js",
  "engines": {
    "node": ">=12.x",
    "npm": "6.x"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "setup-local-env": "./setup-local-env.sh",
    "start-local": "node -r dotenv/config src/server.js",
    "start": "node src/server.js",
    "unit-tests": "nyc --check-coverage --lines 93 --reporter=html --reporter=text mocha ./tests/unit-tests --recursive",
    "integration-tests": "bash ./tests/integration-tests/run.sh",
    "local-integration-tests": "bash ./tests/integration-tests/runLocal.sh --timeout=10000",
    "lint": "eslint src/**",
    "release": "standard-version",
    "test": "npm run lint && npm run unit-tests && npm run local-integration-tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zooz/predator.git"
  },
  "keywords": [
    "performance",
    "framework",
    "scheduler",
    "dsl",
    "kubernetes",
    "metronome"
  ],
  "author": "zooz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zooz/predator/issues"
  },
  "homepage": "https://github.com/Zooz/predator#readme",
  "dependencies": {
    "artillery": "^1.6.0-29",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "copy-dir": "^0.3.0",
    "cron": "1.7.2",
    "dockerode": "^2.5.8",
    "express": "^4.17.1",
    "express-ajv-swagger-validation": "^1.1.1",
    "express-easy-zip": "^1.1.4",
    "express-fileupload": "^1.1.10",
    "express-requests-logger": "^3.0.2",
    "fs-extra": "^5.0.0",
    "graceful-shutdown-express": "^2.0.1",
    "jsck": "^0.3.2",
    "lodash": "^4.17.20",
    "mathjs": "^5.10.3",
    "mysql2": "^2.1.0",
    "node-cmd": "^3.0.0",
    "nodemailer": "^6.4.10",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "pino": "^6.3.2",
    "replace-in-file": "^6.1.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "sequelize": "^5.22.3",
    "shx": "^0.3.2",
    "slack-emojis": "^1.1.1",
    "sqlite3": "^4.2.0",
    "tedious": "^8.3.0",
    "umzug": "^2.3.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@commitlint/cli": "^10.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "chai": "^4.2.0",
    "commitlint": "^10.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^1.3.1",
    "mocha": "^7.1.1",
    "mochawesome": "^4.1.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "rewire": "^5.0.0",
    "should": "^13.2.1",
    "sinon": "^4.5.0",
    "standard-version": "^9.0.0",
    "supertest": "^3.4.2",
    "yaml": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "**/databaseConnector.js",
      "**/database.js"
    ],
    "include": "src"
  }
}
