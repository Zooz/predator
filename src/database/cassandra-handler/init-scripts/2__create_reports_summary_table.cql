CREATE TABLE IF NOT EXISTS reports_summary(
test_id uuid,
revision_id uuid,
report_type text,
report_id text,
status text,
test_configuration text,
last_stats text,
start_time timestamp,
end_time timestamp,
test_name text,
test_description text,
emails list<text>,
webhooks list<text>,
job_id text,
test_type text,
notes text,
phase text,
PRIMARY KEY (report_type, test_id, report_id));
